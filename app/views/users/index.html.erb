<h2> Users </h2>
<div class="flickr_pagination">
    <%= will_paginate @user %>
</div>

<% @user.each do |u| %>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-10">
        Name: <%= link_to u.username, user_path(u) %>, 
        <%= pluralize(u.articles.count, "article") if u.articles %>
      </div>
    </div>
  </ul>
<% end %>
